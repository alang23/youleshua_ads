<script type="text/javascript">
  serverDateObj = new Date(2018, 8, 2, 15, 23, 22);
  window.setTimeout("serverOddsRefresh()", 60000);
  function serverOddsRefresh() {
    serverDateObj.setMinutes(serverDateObj.getMinutes() + 1);
    document.getElementById("server_datetime").innerHTML = formatServerOdds(serverDateObj) ;
    window.setTimeout("serverOddsRefresh()", 60000);
  }
  function formatServerOdds(d) {
    dowArray = new Array('日', '一', '二', '三', '四', '五', '六');
    return addZero(d.getDate()) + "/" + addZero((d.getMonth() + 1)) + "/" + addZero(d.getFullYear()) + " " + addZero(d.getHours()) + ":" + addZero(d.getMinutes())+' ('+dowArray[d.getDay()] + ')';
  }
  function addZero(tstr) {
    return ("0"+tstr).substr((tstr+"").length-1, 2)
  }
  
  var machineId = '72' + ('' == 'sl' ? 's' : '');
  document.getElementById('machineInfo').title = machineId;
</script>

                    
