            $(this).click(function() {
                window.frames["hr_promo_rgp"].WATracker.trackClickEvent('betracing-hr-' + $(this).text().trim().replace(/\s*\/\s*/g, '/').replace(/\s*-\s*/g, '-').replace(/\s/g, '-'));
            });
        });

        function WACommonTagging(type) {
            switch (type) {
                case 'horse':
                    window.frames["hr_promo_rgp"].WATracker.trackClickEvent('hrcommon-hr-horse-01');
                    break;
                case 'jockey':
                    window.frames["hr_promo_rgp"].WATracker.trackClickEvent('hrcommon-hr-jockey-02');
                    break;
                case 'trainer':
                    window.frames["hr_promo_rgp"].WATracker.trackClickEvent('hrcommon-hr-trainer-03');
                    break;
                default:
                    break;
            }
        }
