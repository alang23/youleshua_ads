var ret = 1;
function processBet(toComponent) {
  var noOfBetTypeSelected = 0;
  if(isQRPortal) {
      if($.inArray(1, wpTableList[10].selectLeg['WIN'])>-1 || wpTableList[10].selectField['WIN'] != 0)
        noOfBetTypeSelected++;
      if($.inArray(1, wpTableList[10].selectLeg['PLA'])>-1 || wpTableList[10].selectField['PLA'] != 0)
        noOfBetTypeSelected++;
      if($.inArray(1, wpTableList[10].selectLeg['W-P'])>-1 || wpTableList[10].selectField['W-P'] != 0)
        noOfBetTypeSelected++;
      if(noOfBetTypeSelected>1) {
        alert('Only one betline is allowed');  
        return false;
      }
  }
  var isSuccess = false;
  var betObj1 = processSinglePoolBet(toComponent, 'WIN');
  var betstatus1 = ret;
  var betObj2 = processSinglePoolBet(toComponent, 'PLA');
  var betstatus2 = ret;
  var betObj3 = processSinglePoolBet(toComponent, 'W-P');
  var betstatus3 = ret;
  var betCnt = getNumberOfBet('WIN', betObj1.toBetSel(), '', 10)
             + getNumberOfBet('PLA', betObj2.toBetSel(), '', 10)
             + getNumberOfBet('W-P', betObj3.toBetSel(), '', 10);

  isSuccess = true;
  if ( betCnt > 0 ) {
    if ( toComponent!='betslip' ){
      updateBetCalDisplay(toComponent, betCnt);
      
    }  
  }
  else {
    alert('第#場所選馬匹數目不足。'.replace('#', '10'));  
    isSuccess = false;
  }
  
  if(betstatus1 != 1 || betstatus2 != 1 || betstatus3 != 1){
    isSuccess = false;
  }
  
  return isSuccess;
}

function processSinglePoolBet(toComponent, poolS) {
  var betObj = wpTableList[10].createBetObject(poolS);
  ret = 1;
  if ( toComponent=='betslip' && betObj.legNo>0 ) {
    ret = addToBetslip(poolS, '', '10', betObj.toBetSel(), 1);
    if ( ret==1 ) {
      wpTableList[10].clearCheckBox(poolS);
      wpTableList[10].setInnerTableHtml();
    }
  }
  return betObj;
}

