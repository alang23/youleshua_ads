function initWPTableDetails(tb) {
  tb.horseNumLbl = '馬號';
  tb.horseColorLbl = '綵衣';
  tb.horseNameLbl = '馬名';
  tb.barDrawLbl = '檔位 ';
  tb.handicapLbl = '負磅';
  tb.jockeyLbl = '騎師';
  tb.trainerLbl = '練馬師';
  tb.winLbl = '獨贏';
  tb.plaLbl = '位置';
  tb.wipLbl = '獨贏及位置';
  tb.fieldLbl = '全餐';
}
function setRacingTableObj2(tb) {
    
  var tmp = drawWgtByRace[tb.raceNo].split(';;');
  if ( tmp.length > 2 ) {
      var brandList = tmp[0].split('|');
      var drawList = tmp[1].split('|');
      var handicapList = tmp[2].split('|');

    for ( var i=1; i<brandList.length; i++ ) {
        var brandSub = brandList[i].split('/');
        var drawSub = drawList[i].split('/');
        var handicapSub = handicapList[i].split('/');
        for ( var j=0; j<brandSub.length; j++ ) {
            tb.tableObj[i][j].brand = brandSub[j];
            tb.tableObj[i][j].draw = drawSub[j];
            tb.tableObj[i][j].handicap = handicapSub[j];
        }
    }
  }
  tmp = jockeysByRace[tb.raceNo].split(';;');
  if ( tmp.length > 1 ) {
    var jockeyNameList = tmp[0].split('|');
    var jockeyCodeList = tmp[1].split('|');
    for ( var i=1; i<jockeyNameList.length; i++ ) {
      var jNameSub = jockeyNameList[i].split('/');
      var jCodeSub = jockeyCodeList[i].split('/');
      for ( var j=0; j<jNameSub.length; j++ ) {
        tb.tableObj[i][j].jName = jNameSub[j];
        tb.tableObj[i][j].jCode = jCodeSub[j];
      }
    }
  }
  tmp = trainersByRace[tb.raceNo].split(';;');
  if ( tmp.length > 1 ) {
    var trainerNameList = tmp[0].split('|');
    var trainerCodeList = tmp[1].split('|');
    for ( var i=1; i<trainerNameList.length; i++ ) {
      var tNameSub = trainerNameList[i].split('/');
      var tCodeSub = trainerCodeList[i].split('/');
      for ( var j=0; j<tNameSub.length; j++ ) {
        tb.tableObj[i][j].tName = tNameSub[j];
        tb.tableObj[i][j].tCode = tCodeSub[j];
      }
    }
  }
}

function loadRaceColor() {
  for ( var i=1; i<=wpTableList[10].tableSize; i++ ) {
    var tbObj =  wpTableList[10].tableObj[i];
    for ( var j=0; j<tbObj.length; j++ ) {
      LoadRaceColorImage(tbObj[j].brand);
    }
  }
}

function LoadRaceColorImage(brand) {
    var img = new Image();
    var tmp = document.getElementById('wpTableImg' + brand);            
    img.src = '/content/images/raceColor/' + brand + (isOverseaMeeting ? '' : '.gif?CV=L302R1f');
    img.title = brand;
    img.heigth = 22;
    width = 17;
    if (img.complete) {
      if ( tmp!=null )
        tmp.src = img.src;
    }
    img.onload = function() {
      if ( tmp!=null) {
        tmp.src = img.src;
      }
    }
    img.onerror = function() {
      if ( tmp!=null)
        tmp.src = '/info/include/images/spacer.gif';
    }
}

