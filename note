    function toggleCombinationVisibility(eid, iconid) {
        if ($("#" + eid).is(":visible")) {
            $("#" + eid).hide();
            $("#" + iconid).html("<img src=\"/info/include/images/btn_open.gif\">");
        } else {
            $("#" + eid).show();
            $("#" + iconid).html("<img src=\"/info/include/images/btn_close.gif\">");
        }
    }

    $('.combination').css({ "display": "none", "color": "#948a54" });
</script>   
