var tableSize;
var currentBetline;
var editBetVenue;
var editBetDayOfWeek;
var editBetPoolName;
var editBetLine;
var editBetRace;
var editBetLegNum;
var editBetCurrentUnit;
var editBetIsFlexiBet;

function editBetSplitBetLine(){
    tableSize = horseNames[10].length-1;
    
    //currentBetline = "ST SAT WIN 2*1+2+10".split(" ");
    //editBetCurrentUnit = 10;
    //editBetIsFlexiBet = true;
    currentBetline = top.betSlipFrame.curEditingBetline.split(" ");
    editBetCurrentUnit = top.betSlipFrame.curEditingUnitBet;
    editBetIsFlexiBet = top.betSlipFrame.curEditingIsFlexiBet;
    editBetVenue = (typeof currentBetline[0] == undefined ? '' : currentBetline[0]);
    editBetDayOfWeek = (typeof currentBetline[1] == undefined ? '' : currentBetline[1]);
    editBetPoolName = (typeof currentBetline[2] == undefined ? '' : currentBetline[2]);
    editBetLine = (typeof currentBetline[3] == undefined ? '' : currentBetline[3].split("\*"));
    if(editBetLine != ''){
        editBetRace = editBetLine[0];
        editBetLegNum = editBetLine[1].split("+");
    }
    
    if(!isEditBetAlerted && 
        (editBetVenue != venueShort || editBetDayOfWeek != dayShort 
        || (typeof poolSellStatus[0] != undefined && poolSellStatus[0] == "0")
        || (typeof poolSellStatus[editBetRace] != undefined && poolSellStatus[editBetRace].indexOf(editBetPoolName) == -1))){
        $(document).ready(function() {
            alert('本注項已截止投注。');
            closeEditBetMode();
            isEditBetAlerted = true;
            isEditBetMode = false;
        });
    }
}

function editBetSelectCheckbox(checkOnly){
    if(!checkOnly){
        $("#betAmountCalTopUnitBetInput").val(editBetCurrentUnit);
        $("#betAmountCalBottomUnitBetInput").val(editBetCurrentUnit);
        if(editBetIsFlexiBet){
            selectBetMethod('flexibet');
        }else{
            selectBetMethod('unitbet');
        }
    }
    
    var currentStarter = (typeof startersStatusByRace[editBetRace] == undefined ? '' : startersStatusByRace[editBetRace].split('|'));
    var checkboxNotAvailable = false;
    for(var i = 0 ; i < editBetLegNum.length ; i++){
        if(editBetLegNum[i] == "F"){
            if(!checkOnly){
                wpFieldClick(editBetRace, editBetPoolName);
            }
            break;
        }
        
        if (typeof currentStarter[editBetLegNum[i]] != undefined) {
            var isNotAllScratchOrReserve = false;
            var curStarter = currentStarter[editBetLegNum[i]].split("/");
            for (var k = 0; k < curStarter.length; k++) {
                if (curStarter[k].trim() == "") {
                    isNotAllScratchOrReserve = true;
                    break;
                }
            }

            if (isNotAllScratchOrReserve) {
                if(!checkOnly){
                    wpLegClick(editBetRace, editBetLegNum[i], editBetPoolName);
                }
            } else {
                checkboxNotAvailable = true;
            }
        } 
        
        /*if (typeof currentStarter[editBetLegNum[i]] != undefined &&
            (currentStarter[editBetLegNum[i]].indexOf("S") > -1 || 
            currentStarter[editBetLegNum[i]].indexOf("R") > -1)) {
            checkboxNotAvailable = true;
        }else if(!checkOnly){        
            wpLegClick(editBetRace, editBetLegNum[i], editBetPoolName);
        }*/
    }
    
    if(!isEditBetAlerted && checkboxNotAvailable){
        $(document).ready(function() {
          alert('已加入的選項已不適用，請重新儲入。');
          isEditBetAlerted = true;
        });
    }
}

function openEditOddsContent(){
    $("#wp_bet_cal_top").addClass("editBetHightlightItem");
    $("#wp_bet_cal_top").addClass("editBetBG");
        
    $(".tableContentHead").addClass("editBetHightlightItem");
    $(".info_cell").addClass("editBetHightlightItem");
    
    if(editBetPoolName == 'WIN'){
        $("#lblPla").removeClass("editBetHightlightItem");
        $("#lblWip").removeClass("editBetHightlightItem");
    
        $("#lblWin").addClass("editBetHightlightItem");
        for ( var i=1; i<=tableSize; i++ ) {
            $("#winCell" + i).addClass("editBetHightlightItem");
        }
        $("#winCellF").addClass("editBetHightlightItem");
        
    }else if(editBetPoolName == 'PLA'){
        $("#lblWin").removeClass("editBetHightlightItem");
        $("#lblWip").removeClass("editBetHightlightItem");
    
        $("#lblPla").addClass("editBetHightlightItem");
        for ( var i=1; i<=tableSize; i++ ) {
            $("#plaCell" + i).addClass("editBetHightlightItem");
        }
        $("#plaCellF").addClass("editBetHightlightItem");
        
    }if(editBetPoolName == 'W-P'){
        $("#lblWin").removeClass("editBetHightlightItem");
        $("#lblPla").removeClass("editBetHightlightItem");
    
        $("#lblWip").addClass("editBetHightlightItem");
        for ( var i=1; i<=tableSize; i++ ) {
            $("#wipCell" + i).addClass("editBetHightlightItem");
        }
        $("#wipCellF").addClass("editBetHightlightItem");
        
    }
    
    $("#lblFull").addClass("editBetHightlightItem");
    
    // bugs in ie7 and ie8
    $('.tableContent1').append(" ");
    
    $("#raceInfobtn_addslip").hide();
    $("#raceInfoBottombtn_addslip").hide();
    
    //disable link
    $(".wpTdColor").addClass("linkDisabled");
    $(".wpTdColor").click(function (e){return false;});
}

function closeEditOddsContent(){
    betCalReset('" + poolList + "');

    $("#wp_bet_cal_top").removeClass("editBetHightlightItem");
    $("#wp_bet_cal_top").removeClass("editBetBG");
    
    $(".tableContentHead").removeClass("editBetHightlightItem");
    $(".info_cell").removeClass("editBetHightlightItem");
    
    $("#lblWin").removeClass("editBetHightlightItem");
    $("#lblPla").removeClass("editBetHightlightItem");
    $("#lblWip").removeClass("editBetHightlightItem");
    $("#lblFull").removeClass("editBetHightlightItem");
    for ( var i=1; i<=tableSize; i++ ) {
        $("#winCell" + i).removeClass("editBetHightlightItem");
        $("#plaCell" + i).removeClass("editBetHightlightItem");
        $("#wipCell" + i).removeClass("editBetHightlightItem");
    }
    
    $(".tableContentEnd").removeClass("editBetHightlightItem");
    $("#winCellF").removeClass("editBetHightlightItem");
    $("#plaCellF").removeClass("editBetHightlightItem");
    $("#wipCellF").removeClass("editBetHightlightItem");
    
    $("#raceInfobtn_addslip").show();
    $("#raceInfoBottombtn_addslip").show();
    
    // bugs in ie7 and ie8
    $('.tableContent1').each(function() { 
        $(this).html($.trim($(this).html()));
    });
    
    //enable link
    $(".wpTdColor").removeClass("linkDisabled");
    $(".wpTdColor").unbind("click");
    
    try{
        wpTableList[10].clearCheckBox(editBetPoolName);
        wpTableList[10].setInnerTableHtml();
    }catch(e){}
}

