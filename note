var startTotalInvRefresh = false;

function poolInvPoolTotRefreshOddsOddsPush(poolTotalString) {
  //var stringTest = "153029@@@;WIN=1804284.0;PLA=471018.0;QIN=697767.0;QPL=218408.0;TRI=332132.0;DBL=129320.0;TCE=83780.0@@@;WIN=1117447.0;PLA=857831.0;QIN=266870.0;QPL=190801.0;TRI=229233.0;DBL=373550.0;TCE=16690.0@@@;WIN=287131.0;PLA=325558.0;QIN=598205.0;QPL=739841.0;TRI=59480.0;DBL=77940.0;TCE=232503.0@@@;WIN=114419.0;PLA=99981.0;QIN=73893.0;QPL=357626.0;TRI=36844.0;DBL=52560.0;TCE=334944.0@@@;WIN=181070.0;PLA=163687.0;QIN=267015.0;QPL=45877.0;TRI=18774.0;DBL=4140.0;TCE=18785.0@@@;WIN=38400.0;PLA=20680.0;QIN=151460.0;QPL=145686.0;TRI=10888.0;DBL=116940.0;TCE=172090.0@@@;WIN=17510.0;PLA=18030.0;QIN=48330.0;QPL=37350.0;TRI=4982.0;DBL=53640.0;TCE=121228.0@@@;WIN=6902423.0;PLA=28713.0;QIN=236191.0;QPL=97913.0;TRI=42610.0;TCE=31120.0@@@";
  var tmpArray = poolTotalString.split('@@@');
  var singleTotal = 0;
  var allTotal = 0;
  var grandTotal = 0;
  var singlePoolList = 'WIN;PLA;QIN;QPL;IWN;CWA;CWB;CWC;TCE;TRI;F-F;QTT';
  if(tmpArray.length>0){
      updateRefreshTime(tmpArray[0]);
      var raceNo = parseInt('10');
      if(tmpArray[raceNo] && tmpArray[raceNo] !=''){
          var totalString = tmpArray[raceNo].substr(1);
          var totalArray = totalString.split(';');
          for ( var i=0; i<totalArray.length; i++ ) {
            var node = totalArray[i];
            var pool = node.split('=')[0];
            var investment = node.split('=')[1];
            
            var merged = false;
            // handle merged pool
            // check if win / f-f / qtt is merged pool
            if(pool=="WIN" || pool=="F-F" || pool=="QTT") {
                var cPool = pool;
                if(cPool=="QTT") {
                    cPool = "F-F";
                }
                for(var j=0; j<mergedPoolName.length; j++) {
                    if(mergedPoolName[j][1] == cPool) { // merged -> not show
                        merged = true;
                        break;
                    }
                }
            } else if(parseInt(pool)==pool) {
                // merged pool total value      
                for(var j=0; j<mergedPoolName.length; j++) {
                    if(mergedPoolName[j][0] == pool) { // merged -> not show
                        pool = mergedPoolName[j][1];     
                    }
                }
            }
            
            if(!merged) {
                if(document.getElementById('poolInvPoolTot'+pool))
                    document.getElementById('poolInvPoolTot'+pool).innerHTML = '$ ' + addComma(investment);    
                if ( singlePoolList.indexOf(pool) >= 0)
                  singleTotal += parseInt(investment, 10);
                allTotal += parseInt(investment, 10);
            }
          }
          
          
          if(document.getElementById('poolInvPoolTotSUM') != null)
            document.getElementById('poolInvPoolTotSUM').innerHTML = '<strong class="title">$ ' + addComma(singleTotal.toString()) + '</strong>';
          
          if(document.getElementById('poolInvPoolTotTOTALSUM') != null)
            document.getElementById('poolInvPoolTotTOTALSUM').innerHTML = '<strong class="title">$ ' + addComma(allTotal.toString()) + '</strong>';

        
            if (!startTotalInvRefresh) {
                poolInvAutoRefreshTotalInv();
                startTotalInvRefresh = true;
            }

      }
  }
}

function handleOddPushForPoolTotal(poolTotalString){
   poolInvPoolTotRefreshOddsOddsPush(poolTotalString);
}



