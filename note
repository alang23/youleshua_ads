function poolInvPoolTotLoadDoc() {

  if(isPull || !isConnectedAMS){
    poolInvPoolTotRefreshDoc('/racing/getJSON.aspx?type=pooltot&date=2018-09-02&venue=ST&raceno=10');
  }

}
   

function poolInvPoolTotRefreshDoc(url) {

    $.get(url,function(json){
        poolInvPoolTotRefreshOddsJson(json)}
        ).fail(function(){window.setTimeout('poolInvPoolTotRefreshDoc()', 400);});

}
function poolInvPoolTotRefreshOddsJson(json) {
    var nodes = json.inv;
    var singleTotal = 0;
    var allTotal = 0;
    var oddsTime = json.updateTime;
    updateRefreshTime(oddsTime);
    
    var singlePoolList = 'WIN;PLA;QIN;QPL;IWN;CWA;CWB;CWC;TCE;TRI;F-F;QTT';
    var pid = [];
    for ( var i=0; i<nodes.length; i++ ) {
        var node = nodes[i];
        var pool = node.pool;
        var investment = node.value;
        try{
            $('#poolInvPoolTot'+pool).html('$ ' + addComma(investment));
            if (!isNaN(investment)) {
                if (typeof (node.pid) == 'undefined') {
                    if (singlePoolList.indexOf(pool) >= 0) {
                        singleTotal += parseInt(investment, 10);
                    }
                    allTotal += parseInt(investment, 10);
                } else if (pid.indexOf(node.pid) < 0) {
                    if (singlePoolList.indexOf(pool) >= 0) {
                        singleTotal += parseInt(investment, 10);
                    }
                    allTotal += parseInt(investment, 10);
                    pid.push(node.pid);
                }
            }
        }catch(e){

        }
    }
    $('#poolInvPoolTotSUM').html('<strong class="title">$ ' + addComma(singleTotal.toString()) + '</strong>');
    $('#poolInvPoolTotTOTALSUM').html('<strong class="title">$ ' + addComma(allTotal.toString()) + '</strong>');
    $('#poolInvPoolTotGRANDSUM').html('<strong class="title">$ ' + addComma(json.totalInv) + '</strong>');
}



