function switchAllUpPage() {
  var betObj1 = wpTableList[10].createBetObject('WIN');
  var betObj2 = wpTableList[10].createBetObject('PLA');
  var betObj3 = wpTableList[10].createBetObject('W-P');
  var allUpPool = '';
  var allUpSel = '';
  var selectPoolCnt = 0;
  
  if ( betObj1.legNo > 0 ) {
    allUpPool = 'WIN';
    allUpSel = betObj1.toBetSel();
    selectPoolCnt++;
  }
  if ( betObj2.legNo > 0 ) {
    allUpPool = 'PLA';
    allUpSel = betObj2.toBetSel();
    selectPoolCnt++;
  }
  if ( betObj3.legNo > 0 ) {
    allUpPool = 'W-P';
    allUpSel = betObj3.toBetSel();
    selectPoolCnt++;
  }

  if ( selectPoolCnt > 1 ) {
    alert('過關請只選擇一種投注種類。');
    document.getElementById('allupCheckbox').checked = false;
    return;
  }
  else if ( selectPoolCnt == 1 ) {
    if ( window.localStorage )
      window.localStorage.setItem('alupSel', allUpPool + ' ' + allUpSel);
    else
      window.name = allUpPool + ' ' + allUpSel;
  }

    location.href = '/racing/pages/odds_wp_alup.aspx?lang=ch&date=2018-09-02&venue=ST&raceno=10';
}

function initOddsTable() {
    var tb1 = new WinPlaceCheckBoxObj(10);
    wpTableList[10] = tb1;  
    tb1.id = 'wpTable' + 10;
    tb1.tableId = 'detailWPTable';
    tb1.tableSize = horseNames[10].length-1;
    tb1.startSell = (poolSellStatus[0]=='1') ? 1 : 0;
    tb1.sellStatus = poolSellStatus[10];
    initWPTableDetails(tb1);
    setRacingTableObj1(tb1);
    setRacingTableObj2(tb1);

  if ( winOddsByRace.length > 0 ) {
    var wtmp = winOddsByRace[10];
      if(wtmp)
      {
          var tmp = wtmp.split('#');
          updateRefreshTime(winOddsByRace[0]);

          if(tmp[0])
          {
              var tmpOdds = tmp[0].split(';');
              for ( var j=1; j<tmpOdds.length; j++ ) {
                  var tmpStr = tmpOdds[j].split('=');
                  tb1.winOdds[parseInt(tmpStr[0], 10)] = tmpStr[1];
                  tb1.winColorInd[parseInt(tmpStr[0], 10)] = tmpStr[2];
              }
          }
          
          if(tmp[1])
          {
              tmpOdds = tmp[1].split(';');
              for ( var j=1; j<tmpOdds.length; j++ ) {
                  var tmpStr = tmpOdds[j].split('=');
                  tb1.plaOdds[parseInt(tmpStr[0], 10)] = tmpStr[1];
                  tb1.plaColorInd[parseInt(tmpStr[0], 10)] = tmpStr[2];
              }
          }
          
      }
  }
  tb1.generateTable();
  tb1.setInnerTableHtml();
  //setTimeout("",1000);
  
