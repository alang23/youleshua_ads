function createTable(i, divId, subId) {
  if ( subId==null )
    subId=0;

  var tb1 = new RacingWPTable(i);


  tb1.id = 'wpTable' + (i+subId);
  tb1.tableId = (i+subId);

  tb1.lblChoosePool = '彩池&nbsp;:&nbsp;';
  tb1.lblHorseNum = '馬號';
  tb1.lblBanker = '膽';
  tb1.lblLeg = '腳';
  tb1.lblSel = '選擇';
  tb1.lblHorseName = '馬名';
  tb1.lblWin = '獨贏';
  tb1.lblPla = '位置';
  tb1.lblField = '全餐';
  
  tb1.lblComposite = '組合';
  tb1.lblStartersInclude = '包括馬匹';
  tb1.lblOdds = '賠率';
  tb1.lblScr = '退';       
  tb1.lblCompSel = '組合選擇';

  tb1.fieldSize = horseNames[i].length-1;

  tb1.box1 = (txt1ByRace[i]!=null) ? txt1ByRace[i] : '';
  tb1.box2 = (txt2ByRace[i]!=null) ? txt2ByRace[i] : '';

  setRacingTableObj1(tb1);
  //setCwinTableObj(tb1);

  return tb1;
}



function setRacingTableObj1(tb){
  for ( var i=1; i<startersStatusByRace.length; i++ ) {
    horseStats[i] = startersStatusByRace[i].split('|');
  }
  for ( var i=1; i<horseNames[tb.raceNo].length; i++ ) {
    var hNameSub = horseNames[tb.raceNo][i].split('/');
    var hCodeSub = horseCodes[tb.raceNo][i].split('/');
    var hStatSub = null;
    try{
        hStatSub = horseStats[tb.raceNo][i].split('/');
    }catch(e){}
    
    tb.tableObj[i] = new Array(hNameSub.length);
    for ( var j=0; j<hNameSub.length; j++ ) {
      tb.tableObj[i][j] = new RacingTableObj();
      tb.tableObj[i][j].num = i;
      tb.tableObj[i][j].hName = hNameSub[j];
      tb.tableObj[i][j].hCode = hCodeSub[j];
      if(hStatSub != null){
        tb.tableObj[i][j].hStat = hStatSub[j];
      }
      tb.tableObj[i][j].hScr = horseScratchs[tb.raceNo][i];
    }
  }
}

function loadPoolNames() {
  var longNames = new Array();
  var pools =  'WIN###獨贏;;;WIN###獨贏;;;PLA###位置;;;W-P###獨贏及位置'.split(';;;');
  for ( var i=0; i<pools.length; i++ ) {
    var tmp = pools[i].split('###');
    longNames[tmp[0]] = tmp[1];
  }
  return longNames;
}

function loadLongPoolNames() {
  var longNames = new Array();
  var pools =  'WIN###獨贏;;;WIN###獨贏;;;PLA###位置;;;W-P###獨贏及位置'.split(';;;');
  for ( var i=0; i<pools.length; i++ ) {
    var tmp = pools[i].split('###');
    longNames[tmp[0]] = tmp[1];
  }
  return longNames;
}

    function loadLongPoolNamesOtherLang() {
        var longNames = new Array();
        var pools =  'WIN###Win;;;WIN###Win;;;PLA###Place;;;W-P###Win & Place'.split(';;;');
  for ( var i=0; i<pools.length; i++ ) {
      var tmp = pools[i].split('###');
      longNames[tmp[0]] = tmp[1];
  }
  return longNames;
}

function checkSCR(odd) {
    if(odd=="SCR" || odd=="---") 
        return true;
    else
        return false;
}

