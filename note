function updateStarter_Status(statusStr,raceNo,labelText){
      var tmp = drawWgtByRace[raceNo].split(';;');
      if ( tmp.length > 2 ) {
        var brandList = tmp[0].split('|');
        var status = statusStr.split('|');
        for(var i=1;i<brandList.length;i++){
            if(document.getElementById("wpTableImg"+brandList[i]) && status[i]){
                if(status[i] == 'R'){
                    document.getElementById("wpTableImg"+brandList[i]).parentNode.innerHTML = '('+labelText+')';
                }
            }            
        }
      }
}

